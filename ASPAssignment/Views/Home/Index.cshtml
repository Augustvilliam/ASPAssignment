@using ASPAssignment.ViewModels
@model IEnumerable<Data.Entities.ProjectEntity>
@{
    ViewData["Title"] = "Project Menu";
}

<div class="container">
    <div class="mainpage">
        @Html.Partial("Partials/_Sidebar")
        @Html.Partial("Partials/_Topbar")
        <div class="hero">
            <div id="dynamic-content">
                @Html.Partial("Partials/_ProjectView", Model)
            </div>
        </div>
    </div>
</div>

<!-- Modals -->
@Html.Partial("Partials/Modals/_CreateProjectModal", new ProjectCreateForm())
@Html.Partial("Partials/Modals/_EditProjectModal", new ProjectEditForm())
@Html.Partial("Partials/Modals/_EditMemberModal", new MemberUpdateForm())
@Html.Partial("Partials/Modals/_UserCardPartial")
@Html.Partial("Partials/Modals/_DeleteModal")






